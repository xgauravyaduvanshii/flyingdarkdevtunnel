{
  "name": "flyingdarkdevtunnel",
  "version": "0.1.0",
  "private": true,
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/xgauravyaduvanshii/flyingdarkdevtunnel.git"
  },
  "bugs": {
    "url": "https://github.com/xgauravyaduvanshii/flyingdarkdevtunnel/issues"
  },
  "homepage": "https://github.com/xgauravyaduvanshii/flyingdarkdevtunnel#readme",
  "packageManager": "pnpm@10.29.3",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:resilience": "bash scripts/relay-resilience.sh",
    "test:chaos": "bash scripts/chaos-drill.sh",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "typecheck": "turbo run typecheck",
    "dev:infra": "docker compose -f infra/docker/docker-compose.dev.yml up -d",
    "dev:infra:down": "docker compose -f infra/docker/docker-compose.dev.yml down -v"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^25.2.3",
    "eslint": "^9.20.1",
    "prettier": "^3.5.0",
    "turbo": "^2.4.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.24.1"
  }
}
