    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "echo 'no tests'"
  },
  "dependencies": {
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "typescript": "^5.8.2",
    "eslint": "^10.0.0",
    "typescript-eslint": "^8.24.1",
    "@fdt/tsconfig": "workspace:*",
    "@fdt/eslint-config": "workspace:*"
  }
}
